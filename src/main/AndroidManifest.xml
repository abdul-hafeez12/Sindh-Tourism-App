<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sindhtourism">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lounch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lounch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PointsDetails"></activity>
        <activity android:name=".Gallery" />
        <activity android:name=".MakePlan" />
        <activity android:name=".TouristsPoints" />
        <activity android:name=".UpcomingTrips" />
        <activity android:name=".MainScreen" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>